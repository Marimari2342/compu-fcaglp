      PROGRAM DESVIACION

C     variables declaradas
      REAL Q(100), PROM(100), DESV(100), AUX
      INTEGER N

C     Pedido de datos
      WRITE(*,*) "Ingrese la dimension del vector N"
      READ(*,*) N

C     Desarrollo del ejercicio
      IF (N.GT.0) THEN
         DO I=1,N
            AUX
            DO J=1,N
               AUX=
            PROM(I)=1/N

      ELSE
         WRITE(*,*)"ERROR. La dimensión del vector debe ser mayor a 0."
      END IF
      END PROGRAM
